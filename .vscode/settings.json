{
    "cSpell.ignorePaths": [
        "**/package-lock.json",
        "**/package.json",
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/objects/**",
        ".vscode",
        "tsconfig.json"
    ],
    "cSpell.words": [
        "tixid",
        "ttsc",
        "ttypescript",
        "uuidv"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "node_modules": true
    },
    "browser-preview.startUrl": "localhost:3000",
    "browser-preview.verbose": false,
    "html.format.unformatted": "wbr,%",
    "emmet.includeLanguages": {
        "ejs": "html"
    }
}