{
    "cSpell.ignorePaths": [
        "**/package-lock.json",
        "**/package.json",
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/objects/**",
        ".vscode",
        "tsconfig.json"
    ],
    "cSpell.words": [
        "qind",
        "ttsc"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "node_modules": true
    }
}